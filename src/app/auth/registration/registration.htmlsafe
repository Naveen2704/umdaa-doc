                        <ion-row>
                          <ion-col style="margin-top:-27px;">
                            <div margin >
                              <table class="table table-bordered">
                                <thead>
                                  <tr  style="text-align: center;">
                                    <th scope="col">Days</th>
                                    <th scope="col">Morning Session</th>
                                    <th scope="col">Afternoon Session</th>
                                    <th scope="col">Evening Session</th>
                                  </tr>
                                </thead>
                                <tbody>
                                  <tr style="text-align: center;">
                                    <th scope="row">Monday</th>
                                    <td>
                                      <div *ngIf="iconHide">
                                        <ion-icon name="add"  style="zoom:1.5;" (click)="add('Monday','Morning')"></ion-icon>
                                      </div>
                                      <div class="card" style="border-radius: 25px;" *ngIf="iconHideee">
                                        <div>{{monMorFromTime}} - {{monMorToTime}}</div>
                                      </div>
                                    </td>
                                    <td><ion-icon name="add"  style="zoom:1.5;"></ion-icon></td>
                                    <td><ion-icon name="add"  style="zoom:1.5;"></ion-icon></td>
                                  </tr>
                                  <tr style="text-align: center;">
                                    <th scope="row">Tuesday</th>
                                    <td><ion-icon name="add"  style="zoom:1.5;" (click)="add('Tuesday','Morning')"></ion-icon>  <div>
                                      {{tueFromTime}} - {{tueMorToTime}}</div></td>
                                    <td><ion-icon name="add"  style="zoom:1.5;"></ion-icon></td>
                                    <td><ion-icon name="add"  style="zoom:1.5;"></ion-icon></td>
                                  </tr>
                                  <tr style="text-align: center;">
                                    <th scope="row">Wednesday</th>
                                    <td><ion-icon name="add"  style="zoom:1.5;"></ion-icon>
                                      {{wedMorFromTime}} - {{wedMorToTime}}</td>
                                    <td><ion-icon name="add"  style="zoom:1.5;"></ion-icon></td>
                                    <td><ion-icon name="add"  style="zoom:1.5;"></ion-icon></td>
                                  </tr>
                                  <tr style="text-align: center;">
                                    <th scope="row">Thrusday</th>
                                    <td><ion-icon name="add"  style="zoom:1.5;"></ion-icon>
                                      {{thurMorFromTime}} - {{thurMorToTime}}</td>
                                    <td><ion-icon name="add"  style="zoom:1.5;"></ion-icon></td>
                                    <td><ion-icon name="add"  style="zoom:1.5;"></ion-icon></td>
                                  </tr>
                                  <tr style="text-align: center;">
                                    <th scope="row">Friday</th>
                                    <td><ion-icon name="add"  style="zoom:1.5;"></ion-icon> {{friMorFromTime}} - {{friMorToTime}}</td>
                                    <td><ion-icon name="add"  style="zoom:1.5;"></ion-icon></td>
                                    <td><ion-icon name="add"  style="zoom:1.5;"></ion-icon></td>
                                  </tr>
