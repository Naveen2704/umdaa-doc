
<ion-content >
    <ion-grid class="p-0 container mt-0 d-none d-md-block" >
    <mat-horizontal-stepper [linear]="isLinear" #stepper>
      <mat-step [stepControl]="firstFormGroup" label="Doctor Details">
        <ion-grid class="p-0 container mt-0 d-none d-md-block" >
          <div>
            <ion-card class="card card-1">
            <ion-row>
              <ion-col size-md="2"  class="my_bg_v">
                  <div class="logo">
                    <img class="image"  src="assets/logo/umdaa_logo_1.png">
                  </div>
                  <!-- <div class="support">
                      <ion-text text-center >
                        <h6>Support: info@umdaa.co</h6>
                      </ion-text>
                  </div> -->
              </ion-col>
              <ion-col size-md="10" size-xs="12">
                    <!-- <ion-toolbar>
                        <ion-title class="text-muted font-weight-bold large light">REGISTRATION</ion-title>
                    </ion-toolbar> -->
                  <form  [formGroup]="validations_form" (ngSubmit)="signUp(validations_form.value)">
                       <ion-row>
                              <ion-col >
                                  <div margin >
                                      <ion-item >
                                          <ion-label position="floating">First Name <span>*</span></ion-label>
                                          <ion-input type="text" formControlName="first_name" maxlength="30"  clearInput=true  (keypress)="onKeyPress($event)" [ngClass]="{ 'is-invalid': submitted && f.first_name.errors }" required></ion-input>
                                        </ion-item>
                                        <div class="validation-errors error-message">
                                        <div *ngIf="submitted && f.first_name.errors">
                                            <div *ngIf="f.first_name.errors.required">First Name is required</div>
                                        </div>
                                        </div>
                                    </div>
                              </ion-col>
                                <ion-col>
                                        <div margin>
                                            <ion-item >
                                                <ion-label position="floating">Last Name<span>*</span></ion-label>
                                                <ion-input type="text" formControlName="last_name" maxlength="30"  clearInput=true  (keypress)="onKeyPress($event)" [ngClass]="{ 'is-invalid': submitted && f.last_name.errors }" required></ion-input>
                                              </ion-item>
                                              <div class="validation-errors error-message">
                                                  <div *ngIf="submitted && f.last_name.errors " >
                                                      <div *ngIf="f.last_name.errors.required">Last name is required</div>
                                                  </div>
                                              </div>
                                            </div>
                                </ion-col>
                                  <ion-col>
                                      <div margin >
                                          <ion-item >
                                              <ion-label position="floating">Qualification</ion-label>
                                              <ion-input  formControlName="qualification" type="text"  maxlength="64"   (keypress)="inputValidator($event)"></ion-input>
                                            </ion-item>
                                            
                                          </div>
                                  </ion-col>
                             
                         </ion-row>
                          <ion-row>
                              <ion-col>
                                  <div margin >
                                      <ion-item >
                                          <ion-label position="floating">Mobile Number<span>*</span></ion-label>
                                          <ion-input type="text"  maxlength="10" formControlName="mobile" (keypress)="numberOnly($event)"  [ngClass]="{ 'is-invalid': submitted && f.mobile.errors }" required></ion-input>
                                          
                                      </ion-item>
                                        <div class="validation-errors error-message">
                                        <div *ngIf="submitted && f.mobile.errors" >
                                            <div *ngIf="f.mobile.errors.required">Mobile Number Required</div>
                                            <div *ngIf="f.mobile.errors.minlength">Mobile must be 10 Digits</div>
                                            <!-- //<div *ngIf="f.mobile.errors.pattern">Mobile must be 10 Digits</div> -->
                                        </div>
                                        </div>
                                    </div>
                              </ion-col>
                              <ion-col>
                                  <div margin>
                                      <ion-item >
                                          <ion-label position="floating">Email<span>*</span></ion-label>
                                          <ion-input type="text" formControlName="email" maxlength="64" [ngClass]="{ 'is-invalid': submitted && f.email.errors }" required></ion-input>
                                        </ion-item>
                                        <div class="validation-errors  error-message" >
                                        <div *ngIf="submitted && f.email.errors">
                                            <div *ngIf="f.email.errors.required">Email is required</div>
                                            <div *ngIf="f.email.errors.pattern">Please Enter Valid Email</div>
                                        </div>
                                        </div>
                                      </div>
                              </ion-col>
                              <ion-col>
                                  <div margin>
                                      <ion-item >
                                          <ion-label position="floating">Register Number <span>*</span></ion-label>
                                          <ion-input type="text" formControlName="registration_code" maxLength="32" [ngClass]="{ 'is-invalid': submitted && f.registration_code.errors }" required></ion-input>
                                        </ion-item>
                                        <div class="validation-errors error-message">
                                            <div *ngIf="submitted && f.registration_code.errors " >
                                                <div *ngIf="f.registration_code.errors.required">Register Number is required</div>
                                            </div>
                                        </div>
                                      </div> 
                              </ion-col>
                          </ion-row>
                          <ion-row>
                            <ion-col>
                              <div margin>
                                  <ion-item >
                                      <ion-label position="floating">Password <span>*</span></ion-label>
                                      <ion-input (ionChange)="checkPassword($event)" type="password" formControlName="password" maxLength="32" required></ion-input>
                                    </ion-item>
                                    <div *ngIf="closes">
                                      <div>
                                        <ng-container>
                                          <div class="error-message">
                                            <small>Should Contain Minimum 8 characters,One Captial Letter,One Number,One special Character</small>
                                          </div>
                                        </ng-container>
                                      </div> 
                                    </div>  
                                    <div *ngIf="hidee">
                                      <div>
                                        <ng-container>
                                          <div class="error-message">
                                            <small>Password is Required</small>
                                          </div>
                                        </ng-container>
                                      </div> 
                                    </div> 
                                    <!-- <div *ngIf="hide">
                                      <div>
                                        <ng-container>
                                          <div class="error-message">
                                            <small>One Captial Letter</small>
                                          </div>
                                        </ng-container>
                                      </div> 
                                    </div>  
                                    <div *ngIf="hide">
                                      <div>
                                        <ng-container>
                                          <div class="error-message">
                                            <small>One Number</small>
                                          </div>
                                        </ng-container>
                                      </div> 
                                    </div>   -->
                                  </div> 
                          </ion-col>
                          <ion-col>
                            <div margin>
                                <ion-item >
                                    <ion-label position="floating">Confirm Password <span>*</span></ion-label>
                                    <ion-input  (ionChange)="checkCPassword($event)"  type="password" formControlName="confirm_password" maxLength="32" required></ion-input>
                                  </ion-item>
                                  <div>
                                    <div>
                                      <ng-container *ngIf="hideee">
                                        <div class="error-message">
                                          <small> Confirm Password is Required</small>
                                        </div>
                                      </ng-container>
                                    </div> 
                                  </div>
                                  <div>
                                      <ng-container *ngIf="pConfirm">
                                        <div class="error-message">
                                          <small> Password Not Matched</small>
                                        </div>
                                      </ng-container>
         
                                  </div>
                                </div> 
                        </ion-col>
                        <ion-col>
                          <div margin>
                              <ion-item >
                                <ion-label position="floating">Clinic / Practice Place</ion-label>
                                <ion-select  (ionChange)="checkClincValue($event)"  formControlName="clinic_id" placeholder="Please select clinic">
                                  <ion-select-option value="0">
                                    <ion-text color="danger">Add New Clinic</ion-text> 
                                  </ion-select-option>
                                   <ion-select-option *ngFor="let option of optionsList" value="{{option.clinic_id}}" text-wrap>{{option.clinic_name}}</ion-select-option>
                                 
                                </ion-select>                           
                              </ion-item>
                              
                         </div>
                       </ion-col>  
                          </ion-row>
